  target 'EFPV2Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxNimble'
    pod 'RxBlocking'
    pod 'OHHTTPStubs'
    pod 'OHHTTPStubs/Swift'
    pod 'Quick'
    pod 'RxTest'
   
  end

  target 'EFPV2UITests' do
    inherit! :search_paths
    # Pods for testing
pod 'RxNimble'
pod 'RxBlocking'
pod 'OHHTTPStubs'
pod 'OHHTTPStubs/Swift'
